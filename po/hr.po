msgid "opts can be:\n"
msgstr "opts mogu biti:\n"

#: src/main.c:1943
msgid "-help            : show this help text and exit\n"
msgstr "-help : pokaži ovaj tekst za pomoć i izađi\n"

#: src/main.c:1944
msgid "-tmpdir <tempdir>: use alternate working directory (e.g /var/ramdisk)\n"
msgstr ""

