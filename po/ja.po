#: src/main.c:2117
#, c-format
msgid ""
"\n"
"Startup syntax is: %s [opts] [filename [start_time] [frames]]\n"
msgstr ""
"\n"
"起動方法: %s [オプション] [ファイル名 [開始時刻] [フレーム]]\n"

#: src/main.c:2118
msgid "Where: filename is the name of a media file or backup file.\n"
msgstr ""
"ファイル名は、メディアファイルもしくはバックアップファイルの名前です。\n"

#: src/main.c:2119
msgid "start_time : filename start time in seconds\n"
msgstr "開始時刻 : ファイル名 開始時刻(秒)\n"

#: src/main.c:2120
msgid "frames : maximum number of frames to open\n"
msgstr "フレーム : オープンする最大フレームの数\n"

#: src/main.c:2122
msgid "opts can be:\n"
msgstr "オプション:\n"

#: src/main.c:2123
msgid "-help            : show this help text and exit\n"
msgstr "-help : ヘルプを表示して終了\n"

#: src/main.c:2124
msgid "-tmpdir <tempdir>: use alternate working directory (e.g /var/ramdisk)\n"
msgstr "-tmpdir <tempdir>: 別な作業用ディレクトリを使う(例 /var/ramdisk)\n"

#: src/main.c:2125
msgid "-set <setname>   : autoload clip set setname\n"
msgstr "-set <setname> : 自動読み込みするクリップをsetnameとする\n"

#: src/main.c:2126
msgid "-noset           : do not load any set on startup\n"
msgstr "-noset : 開始時にセットの読み込みをしない\n"

#: src/main.c:2127
msgid "-norecover       : force no-loading of crash recovery\n"
msgstr "-norecover : 読み込み時にクラッシュからの復元を強制しない\n"

#: src/main.c:2128
msgid "-recover         : force loading of crash recovery\n"
msgstr "-recover : 読み込み時にクラッシュからの復元を強制する\n"

#: src/main.c:2129
msgid "-nothreaddialog  : doe nothing - retained for backwards compatibility\n"
msgstr "-nothreaddialog  : 何も行いません - 後方互換性のために残しています\n"

#: src/main.c:2130
msgid "-nogui           : do not show the gui\n"
msgstr "-nogui           : GUIを表示しない\n"

#: src/main.c:2131
msgid "-nosplash        : do not show the splash window\n"
msgstr "-nosplash        : スプラッシュウィンドウを表示しない\n"

#: src/main.c:2132
msgid "-noplaywin       : do not show the play window\n"
msgstr "-noplaywin       : プレイウィンドウを表示しない\n"

#: src/main.c:2133
msgid "-startup-ce      : start in clip editor mode\n"
msgstr "-startup-ce      : クリップ編集モードで起動する\n"

#: src/main.c:2134
msgid "-startup-mt      : start in multitrack mode\n"
msgstr "-startup-mt      : マルチトラックモードで起動する\n"

#: src/main.c:2135
msgid ""
"-fxmodesmax <n>  : allow <n> modes per effect key (minimum is 1, default is "
"8)\n"
msgstr ""
"-fxmodesmax <n>  : エフェクトキーそれぞれに <n> つのモードを許可する(最小は"
"1、デフォルトは8)\n"

#: src/main.c:2137
msgid "-oscstart <port> : start OSC listener on UDP port <port>\n"
msgstr "-oscstart <port> : OSCリスナーをUDPポート<port>で開始する\n"

#: src/main.c:2138
msgid "-nooscstart      : do not start OSC listener\n"
msgstr "-nooscstart : OSCリスナーを起動しない\n"

#: src/main.c:2140
msgid ""
"-aplayer <ap>    : start with selected audio player. <ap> can be mplayer"
msgstr "-aplayer <ap> : 選択したオーディオプレーヤーを起動する。<ap>はmplayer"

#. TRANSLATORS: pulse (audio)
#: src/main.c:2143
msgid ", pulse"
msgstr ", pulse"

#: src/main.c:2146
msgid ", sox or jack\n"
msgstr "、soxまたはjack\n"

#: src/main.c:2147
msgid ""
"-jackopts <opts>    : opts is a bitmap of jack startup options [1 = jack "
"transport client, 2 = jack transport master, 4 = start jack transport "
"server, 8 = pause audio when video paused, 16 = start jack audio server] \n"
msgstr ""
"-jackopts <opts>    : JACKサーバ起動時のオプションのビットマップ[1 = jack "
"transport client, 2 = jack transport master,4 = start jack transport server, "
"8 = pause audio when video paused,16 = start jack audio server] \n"

#: src/main.c:2149
msgid " or sox\n"
msgstr " または sox\n"

#: src/main.c:2151
msgid "-devicemap <mapname>          : autoload devicemap\n"
msgstr "-devicemap <mapname>          : autoload devicemap\n"

#: src/main.c:2152
msgid ""
"-vppdefaults <file>          : load video playback plugin defaults from "
"<file> (Note: only sets the settings, not the plugin type)\n"
msgstr ""
"-vppdefaults <file>          : 映像再生プラグインのデフォルトを<file> から読"
"み込む(注: プラグインの種類ではなく設定のセットのみ)\n"

#: src/main.c:2153
msgid "-debug            : try to debug crashes (requires 'gdb' installed)\n"
msgstr ""
"-debug            : クラッシュのデバックを試みる (gdbのインストールが必要)\n"

